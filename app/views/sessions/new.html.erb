<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

  <%= form_for :session do |f|  %>
    <%= f.label :name %>
      <%= f.text_field :name %>
    <%= f.label :email %>
      <%= f.text_field :email %>
    <%= f.label :password %>
      <%= f.password_field :password %>
    <%= f.submit "take me to my secret stash"%>
  <% end %>